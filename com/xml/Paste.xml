<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baomidou.test.mapper.PasteMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.baomidou.test.entity.Paste">
        <id column="id" property="id" />
        <result column="title" property="title" />
        <result column="subtitle" property="subtitle" />
        <result column="content" property="content" />
        <result column="barid" property="barid" />
        <result column="imgurl" property="imgurl" />
        <result column="time" property="time" />
        <result column="spreadNum" property="spreadNum" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, title, subtitle, content, barid, imgurl, time, spreadNum
    </sql>

</mapper>
