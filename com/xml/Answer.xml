        <result column="raffer_time" property="rafferTime" />
        <result column="tel" property="tel" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, userid, queid, is_answer, is_raffle, ans_time, gift, raffer_time, tel
    </sql>

</mapper>
