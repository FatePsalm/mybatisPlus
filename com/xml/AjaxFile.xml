<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baomidou.test.mapper.AjaxFileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.baomidou.test.entity.AjaxFile">
        <id column="id" property="id" />
        <result column="barId" property="barId" />
        <result column="fileUrl" property="fileUrl" />
        <result column="type" property="type" />
        <result column="creationTime" property="creationTime" />
        <result column="isDel" property="isDel" />
        <result column="requestUrl" property="requestUrl" />
        <result column="sign" property="sign" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, barId, fileUrl, type, creationTime, isDel, requestUrl, sign
    </sql>

</mapper>
