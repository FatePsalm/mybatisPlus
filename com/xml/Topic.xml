<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baomidou.test.mapper.TopicMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.baomidou.test.entity.Topic">
        <id column="id" property="id" />
        <result column="topname" property="topname" />
        <result column="topdetail" property="topdetail" />
        <result column="topstatus" property="topstatus" />
        <result column="startdate" property="startdate" />
        <result column="enddate" property="enddate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, topname, topdetail, topstatus, startdate, enddate
    </sql>

</mapper>
